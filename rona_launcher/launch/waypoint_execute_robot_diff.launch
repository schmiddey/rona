<?xml version="1.0"?>
<launch>
  <!-- waypoint execute -->
  <node name="rona_waypoint_execute_node" pkg="rona_waypoint" type="rona_waypoint_execute_node" output="screen">
    <param name="wp_file_load"              type="string" value="/tmp/waypoints.txt"   />
    <param name="loop_rate"                 type="double" value="10.0"               />
    <param name="wait_duration"             type="double" value="0.5"               />
    <param name="do_loop"                   type="bool"   value="false"               />

  </node>
  <!-- Path controller -->
  <!--node name="rona_move_node" pkg="rona_move" type="rona_move_node" output="screen"-->
  <node name="rona_move_node" pkg="rona_move" type="rona_move_node" output="log">
    <param name="pub_cmd_vel_topic"        type="string" value="/cmd_vel"           />
    <param name="pub_state_topic"          type="string" value="rona/move/state"   />
    <param name="pub_process_topic"        type="string" value="rona/move/process" />
    <param name="sub_path_topic"           type="string" value="rona/move/path"    />
    <param name="sub_ctrl_topic"           type="string" value="rona/move/ctrl"    />
    <param name="map_frame"                type="string" value="map"               />
    <param name="robot_frame"              type="string" value="base_footprint"    />
    <param name="kinematic"                type="string" value="differential"    />
    <param name="loop_rate"                type="double" value="50.0"               />
    <param name="vel_lin_max"              type="double" value="0.8"               />
    <param name="vel_ang_max"              type="double" value="1.2"               />
    <param name="target_radius"            type="double" value="0.25"              />
    <param name="target_radius_final"      type="double" value="0.10"               />
    <param name="ang_reached_range"        type="double" value="0.15"               />  <!--todo try less -->
    <param name="lin_end_approach"         type="double" value="1"               />
    <param name="lin_ctrl_scale"           type="double" value="1.5"               />
    <param name="ang_ctrl_scale"           type="double" value="5.2"               />
    <!-- <param name="lin_slope"                type="double" value="0.4"               /> -->
    <!-- <param name="ang_slope"                type="double" value="0.9"               /> -->
    <param name="min_vel_value"            type="double" value="0.001"             />
    <!-- <param name= "hold_pos"                type="bool"   value="true"              /> -->
    <!-- <param name= "wait_for_rotation"       type="double" value="0.10"              /> -->

    
  </node>

</launch>
